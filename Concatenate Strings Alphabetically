import java.util.*;
public class Main{
    public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	String s,s1,s2;
	s=sc.next();
	s1=sc.next();
	s2="";
	if(s.charAt(0)>=s1.charAt(0)){
	    s2+=s1+s;
	    System.out.println(s2);
	}
	else if(s.charAt(0)<s1.charAt(0)){
	    s2+=s+s1;
	    System.out.println(s2);
	}
    }
}
