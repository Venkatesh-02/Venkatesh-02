import java.util.*;
public class Main{
    public static void main(String[] args) {
	Scanner z=new Scanner(System.in);
	String s=z.nextLine();
	String[] s1=s.split(" ");
	int c=0;
	for(String k:s1){
	    if(k.equals("a") || k.equals("an") || k.equals("the") || k.equals("A") || k.equals("An") || k.equals("The"))
	        c++;
	}
	System.out.println(c);
	}
}
